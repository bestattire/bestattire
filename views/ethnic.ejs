<% include partials/header %>

<div class="container">
	<header class="jumbotron">
			<div class="container">
				<h1>"Elegance never goes out of style"</h1>
				<p>Let's Explore!!</p>
				<p>
					<a class="btn btn-primary btn-large" href="/ethnic/newethnic">add new attire</a>
				</p>
			</div>
	</header>

	<div class="row text-center" style="display:flex; flex-wrap:wrap;">
		<% ethnics.forEach(function(ethnics){ %>
		<div class="col-md-3 col-md-6">
			<div class="thumbnail">
				<img src="<%= ethnics.image %>">
			</div>
			<div class ="caption">
				<h4><%= ethnics.type %><h4>
			</div>
			<p>
				<a href="/ethnic/<%= ethnics._id %>" class="btn btn-primary">More info</a>
			</p>
		</div>
		<% }); %>
	</div>	
</div>


<% include partials/footer %>